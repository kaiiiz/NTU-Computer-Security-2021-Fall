    "from Crypto.Util.number import *\n",
    "\n",
    "p = getPrime(256)\n",
    "q = getPrime(256)\n",
    "n = p * q\n",
    "phi = (p-1) * (q-1)\n",
    "e = 65537\n",
    "d = inverse(e, phi)\n",
    "print(e, n)\n",
    "print(d)"
